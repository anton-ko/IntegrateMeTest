<div class="l-constrained">
  <h1 class="page-title">Competition Dashboard</h1>
  <% if @competitions.any? %>
    <ul>
      <% @competitions.each do |competition| %>
        <li>
            <%= link_to competition.name, competition_entrant_page(competition) %>
        </li>
      <% end %>
    </ul>
  <% else %>
    <p>Seed your data!</p>
  <% end %>
</div>
